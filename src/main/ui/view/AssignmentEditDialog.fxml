<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="250.0" prefWidth="225.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.AssignmentEditDialogController">
   <children>
      <GridPane layoutX="17.0" layoutY="31.0" prefHeight="129.0" prefWidth="192.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="fldTitle" GridPane.columnIndex="1" />
            <TextField fx:id="fldCourse" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Text fx:id="lblTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
            <Text fx:id="lblCourse" strokeType="OUTSIDE" strokeWidth="0.0" text="Course" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btnCancel" layoutX="87.0" layoutY="199.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
      <Button fx:id="btnSubmit" layoutX="157.0" layoutY="199.0" mnemonicParsing="false" onAction="#onSubmit" text="Submit" />
   </children>
</AnchorPane>
